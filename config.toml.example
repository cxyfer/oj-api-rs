# OJ API Configuration

[server]
listen_addr = "0.0.0.0:3000"
admin_secret = "changeme"
graceful_shutdown_secs = 10

[database]
path = "data/data.db"
pool_max_size = 8
busy_timeout_ms = 5000

[gemini]
api_key = ""
# base_url = ""  # optional, for proxy

[gemini.models.embedding]
name = "gemini-embedding-001"
dim = 768
task_type = "SEMANTIC_SIMILARITY"
batch_size = 32

[gemini.models.rewrite]
name = "gemini-2.0-flash"
temperature = 0.3
timeout = 60
max_retries = 2
workers = 8

[crawler]
timeout_secs = 300

[embedding]
timeout_secs = 30
over_fetch_factor = 4
concurrency = 4

[logging]
rust_log = "info"
level = "INFO"
